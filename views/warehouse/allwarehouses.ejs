<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head') %>
</head>
<body class="container">
<header>
  <%- include('../partials/header') %>
</header>
<br>
<form action="/allwarehouses" method="POST">
  <div class="form-group">
    <label>Warehouse :</label>
    <input name="warehouseLocation" type="text" required class="form-control" /> 
  </div>
    <input type="submit" value="Add Warehouse" class="btn btn-primary"/>
  </form>

<br><br>

<% warehouses.forEach(function(w){ %>
  <div class="card-columns">
  <div class="card">
    <div class="card-body">
        <h3><%= w.warehouseLocation  %></h3>
    </div>
  </div>
  </div>
  <% }) %> 

 
  <footer>
    <%- include('../partials/footer') %>
  </footer>
</body>

</html>